<div class="contenedor">
<div class="contenido">
<div class="forma">
<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  
    <div id="nav">
        <% if current_user %>
       
        <%= link_to "Logout", :logout%>
        </div>
       
        <% else %>
        <div id="nav1">
        <%= link_to "Register", new_user_path%> | 
        <div id="nav2">
        <%= link_to "Login", :login %>
        <% end %>
         </div>
          </div>
    </div>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
  <div id="usuario">
<%= link_to 'New User', new_user_path %>
	</div>
</div>
</div>
</div>